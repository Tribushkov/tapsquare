<fest:template xmlns:fest="http://fest.mail.ru">
    <div class="col-md-offset-4 col-md-2">
        <div class="panel panel-info main__panel_mtop">
            <div class="panel-heading">
                <div class="main__header_fsize">ALL</div>
            </div>
            <table class="table table-hover table-condenced">
                <thead>
                    <tr>
                        <th style="text-align:center">Email</th>
                    </tr>
                </thead>
                <tbody id="usersTable" style="text-align:center">
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-md-4">
        <div class="panel panel-info main__panel_mtop">
            <div class="panel-heading">
                <div class="main__header_fsize">Logged</div>
            </div>
            <table class="table table-hover table-condenced">
                <thead>
                    <tr>
                        <th style="text-align:center">Email</th>
                    </tr>
                </thead>
                <tbody id="loggedTable" style="text-align:center">
                </tbody>
            </table>
        </div>
        <div class="row">
            <div class="col-md-offset-6 col-md-2" id="js-back">
                <a class="btn btn-danger" href="/admin?shutdown=1000">SHUT DOWN SERVER</a>
            </div>
        </div>
    </div>
</fest:template>
