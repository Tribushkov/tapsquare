<fest:template xmlns:fest="http://fest.mail.ru">
    <div class="col-md-offset-4 col-md-4">
        <div class="panel panel-default main__panel_mtop">
            <div class="panel-body">
                <form id="registerForm">
                    <div class="form-group" id="emailGroup">
                        <div id="emailNotification"></div>
                        <input type="email" name="email" class="form-control" id="exampleemail" placeholder="Email"></input>
                    </div>
                    <div class="form-group">
                        <input type="text" name="login" class="form-control" id="examplelogin" placeholder="Login"></input>
                    </div>
                    <div class="form-group" id="password1Group">
                        <div id="passwordNotification" ></div>
                        <input type="password" name="password1" class="form-control" id="password1" placeholder="Password"></input>
                    </div>
                    <div class="form-group" id="password2Group">
                        <input type="password" name="password2" class="form-control" id="password2" placeholder="Confirm password"></input>
                    </div>
                    <div class="row">
                        <div class="col-md-1">
                            <button type="submit" class="btn btn-default btn-primary">Register</button>
                        </div>
                        
                    </div>
                </form>
            </div>
        </div>
        <div class="row">
            <div class="col-md-offset-10 col-md-1">
                <a class="btn btn-success" href="#login">Back</a>
            </div>
        </div>
    </div>
</fest:template>
